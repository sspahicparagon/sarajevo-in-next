{
  "name": "sarajevo.in-next",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "next dev",
    "build": "set NODE_ENV=production npx prisma db pull && npx prisma migrate deploy && npx prisma generate && next build",
    "start": "next start",
    "only-build": "next build && next export",
    "lint": "next lint",
    "gcp-build": "npm run build",
    "generate-local": "dotenv -e .env.local -- npx prisma generate",
    "push-local": "dotenv -e .env.local -- npx prisma db push",
    "pull-local": "dotenv -e .env.local -- npx prisma db pull",
    "migrate-local": "dotenv -e .env.local -- npx prisma migrate dev",
    "migrate-local-create": "dotenv -e .env.local -- npx prisma migrate dev --create-only",
    "deploy": "gcloud config set project sarajevoin-next && gcloud app deploy app.yaml --version=0-1-0"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.3",
    "@chakra-ui/react": "^2.4.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@prisma/client": "^4.6.1",
    "@react-google-maps/api": "^2.17.0",
    "@types/cookie": "^0.5.1",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "axios": "^1.1.3",
    "babel-plugin-superjson-next": "^0.4.5",
    "cookie": "^0.5.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.4",
    "firebase-tools": "^11.16.1",
    "framer-motion": "^6.4.3",
    "global": "^4.4.0",
    "google-maps": "^4.3.3",
    "i18next": "^22.1.4",
    "jose": "^4.11.0",
    "next": "12.2.0",
    "next-compose-plugins": "^2.2.1",
    "next-i18next": "^13.0.0",
    "next-images": "^1.8.4",
    "node": "^16.17.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.1.1",
    "react-lazy-load-image-component": "^1.5.6",
    "react-slick": "^0.29.0",
    "sharp": "^0.31.2",
    "superjson": "^1.12.0",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "prisma": "^4.6.1"
  }
}
